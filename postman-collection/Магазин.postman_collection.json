		{
			"name": "должен оформить заказ на домашнее животное",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Время отклика составляет менее 500 мс\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(500);\r",
							"});\r",
							"\r",
							"pm.test(\"код ответа 200\", function() {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"id питомца имеет номер 89430780\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.id).to.eql(89430780);\r",
							"});\r",
							"\r",
							"pm.test(\"питомец оформлен дата\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.shipDate).to.eql(\"2023-04-04T09:28:18.115+0000\");\r",
							"});\r",
							"\r",
							"pm.test(\"питомец оформлен\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.complete).to.eql(true);\r",
							"});\r",
							"\r",
							"pm.test(\"питомец имеет категорию 1\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.petId).to.eql(1);\r",
							"});\r",
							"\r",
							"pm.test(\"питомец имеет категорию статус \", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.status).to.eql(\"available\");\r",
							"});\r",
							""
						],
						"type": "text/javascript"
					}
				}
